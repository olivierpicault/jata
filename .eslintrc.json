{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "standard",
        "plugin:vue/recommended"
    ],
    "rules": {
        "vue/html-closing-bracket-newline": "off",
        "vue/multiline-html-element-content-newline": "off"
    }
}